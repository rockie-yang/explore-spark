{
  "display_name": "pySpark Local",
  "language": "python",
  "argv": [
    "/opt/conda/bin/python",
    "-m",
    "ipykernel",
    "-f",
    "{connection_file}"
  ],
  "env": {
    "PYSPARK_PYTHON": "/opt/conda/bin/python",
    "SPARK_HOME": "/usr/local/spark-2.3.1-bin-hadoop2.7/",
    "PYTHONPATH": "/usr/local/spark-2.3.1-bin-hadoop2.7/lib/py4j-0.10.7-src.zip:/usr/local/spark-2.3.1-bin-hadoop2.7/python/",
    "PYTHONSTARTUP": "/usr/local/spark-2.3.1-bin-hadoop2.7/python/pyspark/shell.py",
    "PYSPARK_SUBMIT_ARGS": "--master local[*] pyspark-shell"
  }
}
